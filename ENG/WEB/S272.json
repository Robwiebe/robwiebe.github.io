” "}]}]},{"data":{"color":"black","source":"The Narrator","time_period":["0s AD"],"unique_sources":["Matthew"]},"nodes":[{"type":"prose","nodes":[{"marks":[{"data":{"book":"Mat","chapter":"19","verse":"15"},"object":"mark","type":"reference"},{"object":"mark","type":"verse"}],"object":"leaf","text":"15"},{"marks":[{"data":{"book":"Mat","chapter":"19","verse":"15"},"object":"mark","type":"reference"}],"object":"leaf","text":"He laid his hands on them, and departed from there. "},{"marks":[{"data":{"book":"Mat","chapter":"19","verse":"16"},"object":"mark","type":"reference"},{"object":"mark","type":"verse"}],"object":"leaf","text":"16"},{"marks":[{"data":{"book":"Mat","chapter":"19","verse":"16"},"object":"mark","type":"reference"}],"object":"leaf","text":"Behold, one came to him and said, "}]}]},{"data":{"color":"blue","recipient":"Jesus","source":"A Rich Young Man","time_period":["0s AD"],"unique_recipients":["Jesus"],"unique_sources":["A Rich Young Man"]},"nodes":[{"type":"prose","nodes":[{"marks":[{"data":{"book":"Mat","chapter":"19","verse":"16"},"object":"mark","type":"reference"}],"object":"leaf","text":"“Good teacher, what good thing shall I do, that I may have eternal life?” "}]}]},{"data":{"color":"black","source":"The Narrator","time_period":["0s AD"],"unique_sources":["Matthew"]},"nodes":[{"type":"prose","nodes":[{"marks":[{"data":{"book":"Mat","chapter":"19","verse":"17"},"object":"mark","type":"reference"}],"object":"leaf","text":"\t"},{"marks":[{"data":{"book":"Mat","chapter":"19","verse":"17"},"object":"mark","type":"reference"},{"object":"mark","type":"verse"}],"object":"leaf","text":"17"},{"marks":[{"data":{"book":"Mat","chapter":"19","verse":"17"},"object":"mark","type":"reference"}],"object":"leaf","text":"He said to him, "}]}]},{"data":{"color":"red","recipient":"A Rich Young Man","source":"Jesus","time_period":["0s AD"],"unique_recipients":["A Rich Young Man"],"unique_sources":["Jesus"]},"nodes":[{"type":"prose","nodes":[{"marks":[{"data":{"book"
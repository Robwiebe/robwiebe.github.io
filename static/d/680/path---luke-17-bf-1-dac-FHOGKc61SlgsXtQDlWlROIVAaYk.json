n":2},"object":"mark","type":"Education"},{"data":{"portion":2},"object":"mark","type":"Religion"},{"data":{"book":"Luk","chapter":"17","verse":"35"},"object":"mark","type":"reference"},{"object":"mark","type":"verse"}],"object":"leaf","text":"35"},{"marks":[{"data":{"portion":1},"object":"mark","type":"Economics"},{"data":{"portion":2},"object":"mark","type":"Education"},{"data":{"portion":2},"object":"mark","type":"Religion"},{"data":{"book":"Luk","chapter":"17","verse":"35"},"object":"mark","type":"reference"}],"object":"leaf","text":"Two women will be grinding flour together at the mill; one will be taken, the other left.*” "}],"object":"text"}],"object":"block","type":"speech"},{"data":{"color":"green","recipient":"Jesus","source":"The Disciples","time_period":["0s AD"],"unique_recipients":["Jesus"],"unique_sources":["The Disciples"]},"nodes":[{"leaves":[{"marks":[{"data":{"portion":1},"object":"mark","type":"Celebration"},{"data":{"portion":2},"object":"mark","type":"Education"},{"data":{"portion":2},"object":"mark","type":"Religion"},{"data":{"book":"Luk","chapter":"17","verse":"37"},"object":"mark","type":"reference"},{"object":"mark","type":"verse"}],"object":"leaf","text":"\t37"},{"marks":[{"data":{"portion":1},"object":"mark","type":"Celebration"},{"data":{"portion":2},"object":"mark","type":"Education"},{"data":{"portion":2},"object":"mark","type":"Religion"},{"data":{"book":"Luk","chapter":"17","verse":"37"},"object":"mark","type":"reference"}],"object":"leaf","text":"“Where will this happen, Lord?”* "}],"object":"text"}],"object":"block","type":"speech"},{"data":{"color":"black","source":"The Narrator","time_period":["0s BC"],"unique_sources":["Luke"]},"nodes":[{"leaves":[{"marks":[{"data":{"portion":1},"object":"mark","type":"Celebration"},{"data":{"portion":2},"object":"mark","type":"Education"},{"data":{"portion":2},"object":"mark","type":"Religion"},{"data":{"book":"Luk","chapter":"17","verse":"37"},"object":"mark","type":"reference"}],"object":"leaf","text":"the disciples asked. \n\tJesus replied, "}],"object":"text"}],"object":"block","type":"speech"},{"data":{"color":"red","recipient":"The Disciples","source":"Jesus","time_period":["0s BC","Os AD"],"unique_recipients":["The Disciples"],"unique_sources":["Jesus"]},"nodes":[{"leaves":[{"marks":[{"data":{"portion":1},"object":"mark","type":"Celebration"},{"data":{"portion":2},"object":"mark","type":"Education"},{"data":{"portion":2},"object":"mark","type":"Religion"},{"data":{"book":"Luk","chapter":"17","verse":"37"},"object":"mark","type":"reference"}],"object":"leaf","text":"“Just as the gathering of vultures shows there is a carcass nearby, so these signs indicate that the end is near.”* "}],"object":"text"}],"object":"block","type":"speech"}],"object":"document"},"object":"value"}}}